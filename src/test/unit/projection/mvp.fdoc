@title Matrx value projections
@felix
// Array
var x : (int ^ 3) ^ 2 = ((1,2,3),(4,5,6));
println$ x;

// Matrix!
var y = x :>> (int^(2\*3));
println$ y;

// inline projection
for i in ..[2] for j in ..[3] perform
  println$ y. (i\,j);

// stand-alone projection
for i in ..[2] for j in ..[3] do
  var p = aproj (i\,j) of int ^ (2 \* 3);
  println$ y . p;
done
@expect
((1, 2, 3), (4, 5, 6))
(1, 2, 3, 4, 5, 6)
1
2
3
4
5
6
